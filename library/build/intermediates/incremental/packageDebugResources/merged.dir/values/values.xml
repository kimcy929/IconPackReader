<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#FC0231</color>
    <color name="colorDialogBackground">@android:color/white</color>
    <color name="colorPrimary">#222228</color>
    <color name="colorPrimaryDark">#1A1A20</color>
    <color name="colorWindowBackground">@android:color/white</color>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <string name="dismiss">DISMISS</string>
    <string name="icon_pack_name_activity_title">List icon pack</string>
    <string name="night_mode">Night mode</string>
    <string name="ok">@android:string/ok</string>
    <string name="search">Search</string>
    <style name="AlertDialogIconPackReaderStyle" parent="Theme.MaterialComponents.DayNight.Dialog.Alert">
        <item name="colorSurface">@color/colorDialogBackground</item>
        <item name="colorPrimary">@color/colorAccent</item>
    </style>
    <style name="BaseIconPackReaderTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="ListIconPackTheme" parent="BaseIconPackReaderTheme">
        <item name="android:windowBackground">@color/colorWindowBackground</item>
        <item name="searchViewStyle">@style/SearchViewStyle</item>
    </style>
    <style name="SearchViewStyle" parent="Widget.AppCompat.Light.SearchView">
        <item name="submitBackground">@color/colorPrimary</item>
        <item name="queryBackground">@color/colorPrimary</item>
        <item name="searchHintIcon">@null</item>
        <item name="searchIcon">@drawable/ic_search_black_24dp</item>
    </style>
</resources>